{
	"name": "LS_ABLB",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"description": "connection to storage accounts on ADLS gen2",
		"parameters": {
			"storageaccount": {
				"type": "string",
				"defaultValue": "quueadevdadlslanding001"
			}
		},
		"annotations": [],
		"type": "AzureBlobStorage",
		"typeProperties": {
			"serviceEndpoint": "@{concat('https://',\nlinkedService().storageaccount,\n'.blob.core.windows.net'\n)}",
			"accountKind": "StorageV2"
		}
	}
}